{
  "name": "interlock",
  "version": "0.7.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jst .",
    "test": "mocha spec/run.js",
    "build": "babel -d lib/ src/ && npm run copy-templates && npm run prep-cli",
    "watch": "watch 'npm run build' src/ -d",
    "copy-templates": "cp -R src/compile/construct/templates lib/compile/construct/",
    "prep-cli": "chmod u+x lib/cli/ilk.js",
    "prepublish": "npm run build",
    "preversion": "npm run check && npm run build",
    "check": "npm run lint && npm run test && ./scripts/validate-docs.sh",
    "generate-docs": "babel-node ./scripts/generate-docs.js"
  },
  "bin": {
    "ilk": "./lib/cli/ilk.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/interlockjs/interlock"
  },
  "author": "Dale Bustad <dale@divmain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/interlockjs/interlock/issues"
  },
  "homepage": "https://github.com/interlockjs/interlock",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-generator": "^6.7.0",
    "babel-polyfill": "^6.3.14",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.5",
    "babel-template": "^6.3.13",
    "babel-traverse": "^6.3.26",
    "babel-types": "^6.4.1",
    "babylon": "^6.7.0",
    "bluebird": "^2.9.34",
    "chokidar": "^1.0.3",
    "lodash": "^3.8.0",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "pluggable": "latest",
    "source-map": "^0.5.0",
    "worker-farm": "^1.3.1",
    "yargs": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.0.5",
    "babel-preset-nodejs-lts": "^1.2.0",
    "chai": "^3.2.0",
    "eslint": "^1.1.0",
    "eslint-config-defaults": "^4.2.0",
    "eslint-plugin-filenames": "^0.1.2",
    "mocha": "^2.2.4",
    "require-dir": "^0.3.0",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.8.0",
    "watch": "^0.17.1"
  }
}
